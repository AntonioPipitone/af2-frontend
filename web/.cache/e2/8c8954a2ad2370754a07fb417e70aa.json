"column":1},"source":"components/Register.js","original":{"line":50,"column":2}},{"generated":{"line":117,"column":15},"source":"components/Register.js","original":{"line":56,"column":17}},{"name":"mapStateToProps","generated":{"line":117,"column":40},"source":"components/Register.js","original":{"line":56,"column":25}},{"generated":{"line":117,"column":55},"source":"components/Register.js","original":{"line":56,"column":17}},{"name":"mapDispatchToProps","generated":{"line":117,"column":57},"source":"components/Register.js","original":{"line":56,"column":42}},{"generated":{"line":117,"column":75},"source":"components/Register.js","original":{"line":56,"column":17}},{"name":"Signup","generated":{"line":117,"column":77},"source":"components/Register.js","original":{"line":56,"column":62}},{"generated":{"line":117,"column":83},"source":"components/Register.js","original":{"line":56,"column":17}}],"sources":{"components/Register.js":"import React, { Component }  from 'react';\nimport {connect} from 'react-redux';\n\nclass Signup extends Component {\n\n  constructor (props){\n    super(props);\n    this.handleSubmit = this.handleSubmit.bind(this);\n  }\n\n  handleSubmit (event) {\n    event.preventDefault();\n    const data = new FormData(event.target) ;\n    const username = data.get('username');\n    const email = data.get('email');\n    const password = data.get('password');\n    console.log(\"form data :\", email + \" ---- \" + password  + \" ---- \" + username);\n    this.props.createUser(email,password,username);\n  }\n\n  render() {\n    return (\n      <div className=\"App\">\n         { this.props.accountStatus.status  ? this.props.accountStatus.message : '' }\n        <h3>Register </h3>\n        <form  onSubmit={this.handleSubmit} autoComplete=\"off\">\n            <label> Username  </label>\n            <input type=\"text\" name=\"username\" id=\"username\" />\n            <br/><br/>\n            <label> Email </label>\n            <input type=\"text\" name=\"email\" id=\"email\" />\n            <br/><br/>\n            <label> Password </label>\n            <input type=\"password\" name=\"password\" id=\"password\" />\n            <br/><br/>\n            <input type=\"submit\" name=\"submit\" id=\"submit\" />\n        </form>\n      </div>\n    );\n  }\n}\n  function mapStateToProps(state) {\n    console.log(\"state => \", state);\n    return {\n      accountStatus : state.user_reducer.accountStatus ?  state.user_reducer.accountStatus : ''\n    };\n  }\n\n\n  const mapDispatchToProps = (dispatch) => ({\n    createUser: (email, password,username) => {\n      dispatch( { type:'ADD_USER' , payload : {email, password,username} } );\n    },\n\n  });\n  export default connect(mapStateToProps, mapDispatchToProps)(Signup);"},"lineCount":null}},"error":null,"hash":"271428b78359a0cf713046ad9825ee2b","cacheData":{"env":{}}}